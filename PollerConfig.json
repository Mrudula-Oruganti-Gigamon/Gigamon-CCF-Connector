{
  "name": "GigamonPushConnectorPolling",
  "apiVersion": "2026-19-01-preview",
  "type": "Microsoft.SecurityInsights/dataConnectors",
  "kind": "Push",
  "properties": {
    "connectorDefinitionName": "GigamonDatasharing",
    "dcrConfig": {
      "streamName": "Custom-GigamonActivity",
      "dataCollectionEndpoint": "{{dataCollectionEndpoint}}",
      "dataCollectionRuleImmutableId": "{{dataCollectionRuleImmutableId}}"
    },
    "auth": {
      "type": "Push",
      "AppId": "{{appId}}",
      "ServicePrincipalId": "{{servicePrincipalId}}"
    },
    "request": {
      "RetryCount":1
    },
    "response": {
      "eventsJsonPaths": [
        "$.messages"
      ]
    }
  }
}